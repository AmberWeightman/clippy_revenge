{
  "name": "Clippy 2: Revenge of the Clip",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Finally, Clippy comes to Chrome! Get help browsing the web.",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "background": {
    "scripts": ["clippy-revenge.js"]
  },
  "browser_action": {
    "name": "Click enable/disable Clippy",
    "default_icon": "icon-active.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css": ["clippy.css"],
      "js": ["jquery-1.8.2.min.js", "clippy.js", "sounds-mp3.js", "agent.js", "clippy-revenge-content.js"]
    }
  ],
  "web_accessible_resources": [
    "map.png"
  ]
}